@model MyCVWebsite.Models.ProjectsViewModel
@{
    ViewData["Title"] = Model.Title;
}

<div class="projects">
    <h1>@Model.Title</h1>
    <ul>
        @foreach (var project in Model.Projects)
        {
            <li>
                <h2>@project.Name</h2>
                <p>@project.Description</p>
                <a href="@project.Link">View Project</a>
            </li>
        }
    </ul>
</div>
